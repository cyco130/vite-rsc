{
	"compilerOptions": {
		"target": "es2020",
		"module": "ESNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"types": ["vite/client", "node"],
		"lib": ["es2021"],
		"skipLibCheck": true,
		"moduleResolution": "Node",
		"resolveJsonModule": true
	}
}
