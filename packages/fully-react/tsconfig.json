{
	"compilerOptions": {
		"target": "es2022",
		"module": "ESNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"types": ["vite/client", "node", "react/experimental"],
		"lib": ["es2021", "dom", "DOM.Iterable"],
		"skipLibCheck": true,
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"jsx": "react-jsx"
	}
}
