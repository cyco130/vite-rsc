{
	"compilerOptions": {
		"target": "es2020",
		"module": "ESNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "Node",
		"types": [
			"vite/client",
			"unplugin-icons/types/react",
			"react/experimental"
		],
		"jsx": "react-jsx",
		"baseUrl": ".",
		"paths": {
			"~/*": ["app/*"]
		}
	}
}
