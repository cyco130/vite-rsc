{
	"compilerOptions": {
		"target": "es2020",
		"module": "ESNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "Node",
		"types": ["vite/client", "react/experimental", "stream-react/types"],
		"jsx": "react-jsx",
		"baseUrl": ".",
		"paths": {
			"~/*": ["app/*"]
		}
	}
}
